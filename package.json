{
  "name": "cash-manager",
  "private": true,
  "type": "module",
  "version": "1.2.12",
  "scripts": {
    "typecheck": "vue-tsc --noEmit",
    "dev": "vite dev --port 1420",
    "dev:android": "npm run tauri android dev",
    "build:tauri:android": "npm run tauri android build",
    "build:vite:develop": "npm run typecheck && vite build --mode development",
    "lint:js": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "lintfix": "prettier --write --list-different . && npm run lint:js --fix && npm run typecheck",
    "build:vite:prod": "npm run typecheck && vite build --mode production",
    "run:stack:windows": "sh build-scripts/run-stack-windows.sh",
    "run:stack:unix": "sh build-scripts/run-stack-unix.sh",
    "tauri": "tauri",
    "rust:install:target:android": "rustup target add aarch64-linux-android armv7-linux-androideabi i686-linux-android x86_64-linux-android"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.0.0-alpha.11",
    "@tauri-apps/plugin-shell": "^2.0.0-alpha.3",
    "@vee-validate/i18n": "^4.11.8",
    "@vee-validate/rules": "^4.11.8",
    "axios": "^1.6.1",
    "html5-qrcode": "^2.3.8",
    "luxon": "^3.4.4",
    "notyf": "^3.10.0",
    "pinia": "^2.1.7",
    "vee-validate": "^4.11.8",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.5.1",
    "@tauri-apps/cli": "^2.0.0-alpha.17",
    "@vitejs/plugin-vue": "^4.4.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-vue": "^9.18.1",
    "internal-ip": "^8.0.0",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "tailwindcss": "^3.3.5",
    "tailwindcss-debug-screens": "^2.2.1",
    "typescript": "^5.1.0",
    "vite": "^4.5.0",
    "vue-tsc": "^1.8.22"
  }
}
